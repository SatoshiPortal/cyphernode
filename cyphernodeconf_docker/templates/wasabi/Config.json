{
<% if (net === 'regtest') { %>
  "Network": "RegTest",
<% } else if (net === 'testnet') { %>
  "Network": "TestNet",
<% } else if (net === 'mainnet') { %>
  "Network": "Main",
<% } %>
  "MainNetBackendUri": "https://api.wasabiwallet.io/",
  "TestNetClearnetBackendUri": "http://wasabi-backend:37127/",
  "RegTestBackendUri": "http://wasabi-backend:37127/",
  "UseTor": <%= (net === 'regtest' || net === 'testnet') ? 'false' : 'true' %>,
  "TerminateTorOnExit": false,
  "DownloadNewVersion": false,
  "StartLocalBitcoinCoreOnStartup": false,
  "StopLocalBitcoinCoreOnShutdown": false,
  "LocalBitcoinCoreDataDir": "/root/.bitcoin",
  "MainNetBitcoinP2pEndPoint": "bitcoin:8333",
  "TestNetBitcoinP2pEndPoint": "bitcoin:18333",
  "RegTestBitcoinP2pEndPoint": "bitcoin:18444",
  "MixUntilAnonymitySet": <%= wasabi_mixuntil %>,
  "PrivacyLevelSome": 2,
  "PrivacyLevelFine": 2,
  "PrivacyLevelStrong": <%= wasabi_mixuntil %>,
  "JsonRpcServerEnabled": true,
  "JsonRpcUser": "<%= wasabi_rpcuser %>",
  "JsonRpcPassword": "<%= wasabi_rpcpassword %>",
  "JsonRpcServerPrefixes": [
    "http://*:18099/"
  ],
  "DustThreshold": "0.00000001",
  "EnableGpu": true,
  "CoordinatorIdentifier": "CoinJoinCoordinatorIdentifier"
}
